FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho.orig;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    wedge1
    {
        type            wedge;
    }
    wedge2
    {
        type            wedge;
    }
    defaultFaces
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
}

