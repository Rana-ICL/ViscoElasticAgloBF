FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "system";
  object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.RDX 0
    volVectorFieldValue U (0 0 0)
);

regions
(
    cylinderToCell
    {
        p1 (0 0 0.005);
        p2 (0 0 1);
        radius 0.0125;
        fieldValues
        (
            volScalarFieldValue alpha.RDX 1
        );
    }
    cylinderToCell
    {
        p1 (0 0 0.005);
        p2 (0 0 0.006);
        radius 0.0125;
        fieldValues
        (
            volVectorFieldValue U (0 0 90)
        );
    }
);

